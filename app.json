{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/icon.png"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "ios": {
      "buildNumber": "2",
      "bundleIdentifier": "com.mizumura.instagpt",
      "supportsTablet": true
    },
    "name": "insta-gpt",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your camera to let you take photos and videos.",
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "scheme": "insta-gpt",
    "slug": "insta-gpt",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
